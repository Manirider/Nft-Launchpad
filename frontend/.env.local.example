# Frontend Environment Variables

# Network Configuration
NEXT_PUBLIC_CHAIN_ID=1
NEXT_PUBLIC_CHAIN_NAME=mainnet

# Contract Configuration
NEXT_PUBLIC_CONTRACT_ADDRESS=0x...

# Wallet Configuration (WalletConnect)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_project_id_here

# IPFS Configuration (for Merkle tree data)
NEXT_PUBLIC_MERKLE_TREE_URL=/merkle-tree.json

# Feature Flags
NEXT_PUBLIC_ENABLE_TESTNET=false

# RPC Endpoints (optional, if not using RainbowKit default)
NEXT_PUBLIC_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_KEY

# Analytics (optional)
NEXT_PUBLIC_GA_ID=
